{
  "name": "RefreshView",
  "vendor": "PrivacySafe",
  "appId": "com.refreshview.browser",
  "binaryName": "refreshview-browser",
  "version": {
    "product": "firefox",
    "version": "132.0.2"
  },
  "buildOptions": {
    "windowsUseSymbolicLinks": false
  },
  "addons": {
    "ublock": {
      "platform": "github",
      "id": "uBlock0@raymondhill.net",
      "repo": "gorhill/uBlock",
      "version": "1.61.0",
      "fileGlob": "uBlock0_*.firefox(.signed)?.xpi"
    },
    "SidebarTabs": {
      "platform": "github",
      "id": "sidebartabs@jaduastudios.com",
      "repo": "Jadua1010/SidebarTabs",
      "version": "v0.6.6",
      "fileGlob": "release.xpi"
    },
    "GoPanic": {
      "id": "gopanic@jaduastudios.com",
      "platform": "github",
      "repo": "JaduaStudios/GoPanic",
      "version": "v0.1.1",
      "fileGlob": "release.xpi"
    },
    "BetterTyping": {
      "id": "bettertyping@jaduastudios.com",
      "platform": "github",
      "repo": "JaduaStudios/BetterTyping",
      "version": "v0.1.3",
      "fileGlob": "release.xpi"
    },
    "GoViewer": {
      "id": "goviewer@jaduastudios.com",
      "platform": "github",
      "repo": "JaduaStudios/GoViewer",
      "version": "v0.2.1",
      "fileGlob": "release.xpi"
    },
    "LeSpoof": {
      "id": "lespoof@jaduastudios.com",
      "platform": "github",
      "repo": "JaduaStudios/LeSpoof",
      "version": "v0.1.0",
      "fileGlob": "release.xpi"
    }
  },
  "brands": {
    "dev": {
      "backgroundColor": "#021327",
      "brandShorterName": "RefreshView",
      "brandShortName": "RefreshView Browser",
      "brandFullName": "RefreshView Browser Dev",
      "release": {
        "displayVersion": "0.5.0-0",
        "github": {
          "repo": "seandiggity/refreshview-browser"
        },
        "x86": {
          "windowsMar": "windows.mar",
          "macosMar": "macos.mar",
          "linuxMar": "linux.mar"
        }
      }
    }
  },
  "license": {
    "ignoredFiles": [
      ".*\\.json"
    ],
    "licenseType": "MPL-2.0"
  },
  "updateHostname": "goupdate.jaduastudios.com"
}
